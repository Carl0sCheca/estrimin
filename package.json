{
  "name": "estrimin-web",
  "version": "2.2.0",
  "private": true,
  "target": "ES2017",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "test": "vitest",
    "generate": "npx prisma generate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "startup": "tsx src/scripts/startup.ts",
    "buildandstart": "npx prisma generate && npx prisma migrate deploy && tsx src/scripts/startup.ts && next build && next start",
    "build:scheduler": "npx prisma generate && esbuild scheduler/src/index.ts --bundle --platform=node --format=esm --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\" --outfile=dist/scheduler/index.js --external:@prisma/client --external:zeromq --tsconfig=scheduler/tsconfig.json",
    "start:scheduler": "node ./dist/scheduler/index.js",
    "dev:scheduler": "tsx watch scheduler/src/index.ts",
    "buildandstart:scheduler": "npm run build:scheduler && npm run start:scheduler"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.995.0",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "7.4.1",
    "@tanstack/react-query": "^5.90.21",
    "better-auth": "^1.4.18",
    "dotenv": "^17.3.1",
    "ffmpeg-stream": "^1.0.1",
    "mp4box": "^2.3.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-icons": "^5.5.0",
    "toad-scheduler": "^3.1.0",
    "uuid": "^13.0.0",
    "whep-player": "git://github.com/Carl0sCheca/whep-player-lib.git#fd34c0ab23a4a06b474f329cbc30ea81d96cd4e9",
    "zeromq": "^6.5.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@tailwindcss/forms": "^0.5.11",
    "@tailwindcss/postcss": "^4.2.0",
    "@types/node": "^24.10.9",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "baseline-browser-mapping": "^2.10.0",
    "esbuild": "^0.27.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "prisma": "^7.4.1",
    "tailwindcss": "^4.2.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0",
    "vite-tsconfig-paths": "^6.1.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3"
  },
  "packageManager": "pnpm@10.29.3+sha512.498e1fb4cca5aa06c1dcf2611e6fafc50972ffe7189998c409e90de74566444298ffe43e6cd2acdc775ba1aa7cc5e092a8b7054c811ba8c5770f84693d33d2dc"
}
