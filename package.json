{
  "name": "estrimin-web",
  "version": "2.0.0",
  "private": true,
  "target": "ES2017",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- next dev",
    "dev:turbo": "dotenvx run -- next dev --turbopack --turbo",
    "generate": "npx prisma generate",
    "build": "dotenvx run -- next build",
    "start": "dotenvx run -- next start",
    "buildandstart": "dotenvx run -- npx prisma migrate deploy && tsx src/scripts/startup.ts && next build && next start",
    "lint": "next lint",
    "startup": "dotenvx run -- tsx src/scripts/startup.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "better-auth": "^1.3.8",
    "ffmpeg-stream": "^1.0.1",
    "mp4box": "^2.1.1",
    "next": "15.5.2",
    "next-themes": "^0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.5.0",
    "toad-scheduler": "^3.1.0",
    "uuid": "^12.0.0",
    "whep-player": "git://github.com/Carl0sCheca/whep-player-lib.git#fd34c0ab23a4a06b474f329cbc30ea81d96cd4e9",
    "zeromq": "^6.5.0",
    "zod": "^4.1.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.49.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^22.17.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "prisma": "^6.15.0",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9"
  }
}
