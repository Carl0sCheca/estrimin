{
  "name": "estrimin-web",
  "version": "1.0.0",
  "private": true,
  "target": "ES2017",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- next dev --turbopack --turbo",
    "generate": "npx prisma generate",
    "build": "dotenvx run -- next build",
    "start": "dotenvx run -- next start",
    "buildandstart": "dotenvx run -- npx prisma migrate deploy && tsx src/scripts/startup.ts && next build && next start",
    "lint": "next lint",
    "startup": "dotenvx run -- tsx src/scripts/startup.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "better-auth": "^1.2.4",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0",
    "whep-player": "git://github.com/Carl0sCheca/whep-player-lib.git#97dc45a520a52c85aca41630cadc7bfd3168ee42",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.39.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.0.17",
    "@types/node": "^22.13.13",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "eslint": "^8",
    "eslint-config-next": "15.2.4",
    "postcss": "^8",
    "prisma": "^6.5.0",
    "tailwindcss": "^4.0.9",
    "tsx": "^4.19.1",
    "typescript": "^5"
  },
  "overrides": {
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4"
  }
}
