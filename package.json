{
  "name": "estrimin-web",
  "version": "1.0.0",
  "private": true,
  "target": "ES2017",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- next dev --turbopack --turbo",
    "generate": "npx prisma generate",
    "build": "dotenvx run -- next build",
    "start": "dotenvx run -- next start",
    "buildandstart": "dotenvx run -- npx prisma migrate deploy && tsx src/scripts/startup.ts && next build && next start",
    "lint": "next lint",
    "startup": "dotenvx run -- tsx src/scripts/startup.ts"
  },
  "dependencies": {
    "@eyevinn/webrtc-player": "^0.13.0",
    "@prisma/client": "^6.2.1",
    "better-auth": "^1.1.14",
    "next": "15.1.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.4.0",
    "uuid": "^11.0.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.33.0",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^22.8.0",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.1.5",
    "postcss": "^8",
    "prisma": "^6.2.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.1",
    "typescript": "^5"
  },
  "overrides": {
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3"
  }
}
