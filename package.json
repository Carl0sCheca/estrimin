{
  "name": "estrimin-web",
  "version": "1.0.0",
  "private": true,
  "target": "ES2017",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -- next dev",
    "dev:turbo": "dotenvx run -- next dev --turbopack --turbo",
    "generate": "npx prisma generate",
    "build": "dotenvx run -- next build",
    "start": "dotenvx run -- next start",
    "buildandstart": "dotenvx run -- npx prisma migrate deploy && tsx src/scripts/startup.ts && next build && next start",
    "lint": "next lint",
    "startup": "dotenvx run -- tsx src/scripts/startup.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.9.0",
    "better-auth": "^1.2.8",
    "ffmpeg-stream": "^1.0.1",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "uuid": "^11.1.0",
    "whep-player": "git://github.com/Carl0sCheca/whep-player-lib.git#fd34c0ab23a4a06b474f329cbc30ea81d96cd4e9",
    "zod": "^3.25.50"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.44.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^22.15.29",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.4",
    "prisma": "^6.9.0",
    "tailwindcss": "^4.1.7",
    "tsx": "^4.19.4",
    "typescript": "^5.6.3"
  },
  "overrides": {
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5"
  }
}
